load sampling
load example

mod SAMPLING-EXT is
  inc SAMPLING + OBJS .
  
  vars O O' TB : Oid .  var QS : Queries .
  var OS : Oids .  var R : Map{Key,Oids} .
  vars AS AS' : Attributes .  var K : Key .
  var D : Map{Key,Value} .  vars V V' : Value .
  var P : Payload .  var DL : Float .
  var OBJS : Objects .  var MSGS : Msgs .
  
  op u-issue : Oid Key Queries Map{Key,Oids} Attributes Float -> Float .
  eq u-issue(O,K,QS,R,AS,DL) = DL .
  
  op sample-issue : Oid Key Queries Map{Key,Oids} Attributes -> Float .
  eq sample-issue(O,K,QS,R,AS) = 1.0 .
  
  op u-reply : Oid Oid Key Map{Key,Value} Attributes Float -> Float .
  eq u-reply(O,O',K,D,AS,DL) = DL .
  
  op sample-reply : Oid Oid Key Map{Key,Value} Attributes -> Float .
  eq sample-reply(O,O',K,D,AS) = 1.0 . 
  
  op u-init : Objects Msgs Float -> Float .
  eq u-init(OBJS,MSGS,DL) = DL .
 
  op sample-init : Objects Msgs -> Float .
  eq sample-init(OBJS,MSGS) = sampleExp(1.0) .
endm